<?xml version="1.0" encoding="utf-8"?>
<LenovoWelcomeSubscription>
	<AppSettingList>

		<!-- Settings -->
		<!-- Package should not run if the current version is less than -->
		<AppSetting key="WelcomeApp.MinimumVersionRequired">2.2.1.6</AppSetting>

		<!--Configure whether LenovoWelcome should be launched-->
		<AppSetting key="WelcomeApp.RunAfterOOBE">true</AppSetting>

    <!--Configure whether Software Entitlement Tile on All Done page should be shown-->
		<AppSetting key="WelcomeApp.ShowSoftwareEntitled">true</AppSetting>

		<!--Configure which version of LenovoWelcome should skip auto-update -->
		<AppSetting key="WelcomeApp.SkipAutoUpdate">2.2.1.6</AppSetting>

		<!--  After this number of days, do not show  -->
		<AppSetting key="WelcomeApp.Timing.MaximumDaysAfterOobe">7</AppSetting>

		<!-- Create scheduled task to run this many minutes after oobe -->
		<AppSetting key="WelcomeApp.Timing.MinimumMinutesAfterOobe">0</AppSetting>

		<!-- Whether or not dropbox is the chosen storage provider -->
		<AppSetting key="WelcomeApp.Features.Storage.DropBox" country="cn">false</AppSetting>
		<AppSetting key="WelcomeApp.Features.Storage.DropBox">true</AppSetting>

		<!-- Whether or not enable Welcome and companion page -->
		<AppSetting key="WelcomeApp.Features.AnimationPage.Enable">true</AppSetting>
		<AppSetting key="WelcomeApp.Features.WelcomePage.Enable">true</AppSetting>
    <AppSetting key="WelcomeApp.Features.PremiumCarePage.Enable">true</AppSetting>
		<AppSetting key="WelcomeApp.Features.LenovoIDPage.Enable">false</AppSetting>
		<AppSetting key="WelcomeApp.Features.AppsForYouPage.Enable">true</AppSetting>		
		<AppSetting key="WelcomeApp.Features.CompanionPage.Enable">true</AppSetting>
		<AppSetting key="WelcomeApp.Features.VantagePage.Enable">false</AppSetting>
		<AppSetting key="WelcomeApp.Features.DropBoxPage.Enable">true</AppSetting>
		<AppSetting key="WelcomeApp.Features.MigratePage.Enable">true</AppSetting>
    <!-- Add checkbox for "Add a shortcut to my desktop during installation" for LMA	 -->
    <AppSetting key="WelcomeApp.Features.MigratePage.MigrateCheckbox.Enable">true</AppSetting>
		<AppSetting key="WelcomeApp.Features.AllDonePage.Enable">true</AppSetting>

	  <AppSetting key="WelcomeApp.Features.AppsForYouPage.amazonAssistant.Enable">true</AppSetting>
	  <AppSetting key="WelcomeApp.Features.AppsForYouPage.winzip.Enable">true</AppSetting>
    <AppSetting key="WelcomeApp.Features.AppsForYouPage.kingsoftWPSOffice.Enable">false</AppSetting>
    <AppSetting key="WelcomeApp.Features.AppsForYouPage.facebook.Enable">true</AppSetting>
    <AppSetting key="WelcomeApp.Features.AppsForYouPage.deezer.Enable">true</AppSetting>
    <AppSetting key="WelcomeApp.Features.AppsForYouPage.cyberlinkMediaPlayer.Enable">false</AppSetting>
    <AppSetting key="WelcomeApp.Features.AppsForYouPage.youTube.Enable">true</AppSetting>
    <AppSetting key="WelcomeApp.Features.AppsForYouPage.alexaForPC.Enable">true</AppSetting>
     
    <!--New AppSettings for LenovoFirstRunExperience-->
		<AppSetting key="LenovoFirstRunExperience.Timing.TimeFrame1.Percentage">0.2</AppSetting>
		<AppSetting key="LenovoFirstRunExperience.Timing.TimeFrame1.MinutesAfterOOBE">0</AppSetting>

		<AppSetting key="LenovoFirstRunExperience.Timing.TimeFrame2.Percentage">0.3</AppSetting>
		<AppSetting key="LenovoFirstRunExperience.Timing.TimeFrame2.MinutesAfterOOBE">2</AppSetting>

		<AppSetting key="LenovoFirstRunExperience.Timing.TimeFrame3.Percentage">0.5</AppSetting>
		<AppSetting key="LenovoFirstRunExperience.Timing.TimeFrame3.MinutesAfterOOBE">5</AppSetting>

	</AppSettingList>
