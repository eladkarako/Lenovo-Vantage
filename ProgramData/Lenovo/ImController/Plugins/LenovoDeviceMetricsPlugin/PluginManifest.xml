<?xml version="1.0" encoding="utf-8"?>
<PluginManifest>
    <PackageInformation name="LenovoDeviceMetricsPlugin" version="1.0.10.0" location="LenovoDeviceMetricsPlugin.cab" lastModified="2016-09-01T21:30:12" customEventMonitor="">
    </PackageInformation>
    <ApplicabilityFilter>
      <!--Enable for any of the dogfooding systems -->
      <Filter tag="DeviceMetricsPlugin.Enablement">true</Filter>

      <!--Hide for systems that specifically have their family set to “Thinkpad Edge” -->
      <Filter family="thinkpad edge*">false</Filter>
     
      <!--Enable for any of the E-series laptops, E465 for example -->
      <Filter family="thinkpad e*">true</Filter>
      
      <!-- Enable for all Lenovo / Idea -->
      <Filter brand="idea">true</Filter>
      <Filter brand="lenovo">true</Filter>
      
      <!--Hide for all other systems -->
      <Filter>false</Filter>
    </ApplicabilityFilter>
    <ContractMappingList>
  </ContractMappingList>
    <EventSubscriptionList>
        <!-- for active directory & machine information & app tag -->
      <EventSubscription monitor="TimeBasedMonitor" trigger="TimeBaseChange" runas="User">
        <Parameter> <![CDATA[<?xml version="1.0" encoding="utf-16"?>
<TimeBasedEventSubscription>
<FriendlyName>RunDailyEvent</FriendlyName>
<OOBEProximity>false</OOBEProximity>
<RepeatIntervalUnit>Daily</RepeatIntervalUnit>
<RepeatInterval>1</RepeatInterval>
</TimeBasedEventSubscription>]]> </Parameter>
      </EventSubscription>
      <EventSubscription monitor="UsageMonitor" trigger="App">
        <Parameter>
          <![CDATA[ ]]>
        </Parameter>
      </EventSubscription>
      <EventSubscription monitor="SystemEventMonitor" trigger="User-Login" runas="User">
        <Parameter><![CDATA[ ]]></Parameter>
      </EventSubscription>
      <EventSubscription monitor="WindowMessageMonitor" trigger="PowerChange">
       <Parameter><![CDATA[<?xml version="1.0" encoding="utf-16"?>
<PowerBroadcastEventSubscription>
<PbtValueList>
<PbtValue>18</PbtValue>
</PbtValueList>
<PowerSettingGuidList />
</PowerBroadcastEventSubscription>]]> </Parameter>
       </EventSubscription>
    </EventSubscriptionList>
  <SettingList>
    <Setting key="ImController.Privilege.CustomKeepAlive">1</Setting>
    <Setting key="ImController.Privilege.SkipSuspendTermination">1</Setting>
  </SettingList>
